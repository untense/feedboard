# Taostats API Configuration
TAOSTATS_API_KEY=your_api_key_here
TAOSTATS_API_URL=https://api.taostats.io

# Cache Configuration
# Cache TTL in milliseconds (default: 30000ms = 30 seconds)
# Taostats API has a rate limit of 60 calls/minute, caching improves performance and reduces API calls
CACHE_TTL=30000

# Server Configuration
PORT=3000
NODE_ENV=development
